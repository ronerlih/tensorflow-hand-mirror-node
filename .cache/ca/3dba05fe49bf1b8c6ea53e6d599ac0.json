olumn":0},"source":"utils/drawing.js","original":{"line":39,"column":2}}],"sources":{"utils/drawing.js":"\nimport {fingerLookupIndices} from './fingers.js';\n\nexport function drawPoint(ctx, y, x, r) {\n   ctx.beginPath();\n   ctx.arc(x, y, r, 0, 2 * Math.PI);\n   ctx.fill();\n }\n\nexport function drawKeypoints(ctx, keypoints) {\n   const keypointsArray = keypoints;\n \n   for (let i = 0; i < keypointsArray.length; i++) {\n     const y = keypointsArray[i][0];\n     const x = keypointsArray[i][1];\n     drawPoint(ctx, x - 2, y - 2, 3);\n   }\n \n   const fingers = Object.keys(fingerLookupIndices);\n   for (let i = 0; i < fingers.length; i++) {\n     const finger = fingers[i];\n     const points = fingerLookupIndices[finger].map(idx => keypoints[idx]);\n     drawPath(ctx, points, false);\n   }\n }\n \n export function drawPath(ctx, points, closePath) {\n   const region = new Path2D();\n   region.moveTo(points[0][0], points[0][1]);\n   for (let i = 1; i < points.length; i++) {\n     const point = points[i];\n     region.lineTo(point[0], point[1]);\n   }\n \n   if (closePath) {\n     region.closePath();\n   }\n   ctx.stroke(region);\n }"},"lineCount":null}},"error":null,"hash":"58ed6015c3691ea0bfe3c8200bc10b7f","cacheData":{"env":{}}}